    static int[] rellenaPares(int longitud, int fin) {
        int pares[] = new int[longitud];
        
        for (int i = 0; i < pares.length; i++) {
            int num = (int) (Math.random() * fin + 1);
            if (num % 2 == 0) {
                pares[i] = num;
                i++;
            }
        }
        Arrays.sort(pares);
        return (pares);
    }
}
