    public static void main(String[] args) {
        int tabla[] = new int[10];
        for (int i = 0; i < tabla.length; i++) {
            tabla[i] = (int) (Math.random() * 100 + 1);
            int suma = 0;
            for (int valor : tabla) {
                suma += valor;
            }
            System.out.println("La suma de los valores aleatorios es " + suma);
        }
    }
}
